{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "{{ project_name_snake }}",
  "version": "0.1.0",
  "identifier": "com.{{ tauri_user_name }}.{{ project_name_kebab }}",
  "build": {
    "beforeDevCommand": "bun run start",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist/angular/browser"
  },
  "app": {
    "windows": [
      {
        "title": "{{ project_name_snake }}",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
